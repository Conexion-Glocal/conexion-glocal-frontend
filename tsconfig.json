{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@resources/*": [
        "./src/resources/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}